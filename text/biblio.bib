@Article{Hall.Carroll1989,
  Title                    = {Variance Function Estimation in Regression: The Effect of Estimating the Mean},
  Author                   = {Peter Hall and R. J. Carroll},
  Journal                  = {Journal of the Royal Statistical Society. Series B (Methodological)},
  Year                     = {1989},
  Number                   = {1},
  Pages                    = {3-14},
  Volume                   = {51},
  Abstract                 = {We consider estimation of a variance function g in regression problems. Such estimation requires simultaneous estimation of the mean function f. We obtain clear results on the extent to which the smoothness of f influences best rates of convergence for estimating g. For example, in nonparametric regression with two derivatives on g, `classical' rates of convergence are possible if and only if the unknown f satisfies a Lipschitz condition of order 1/3 or more. If a parametric model is known for g, then g may be estimated n<sup>1/2</sup> consistently if and only if f is Lipschitz of order 1/2 or more. Optimal rates of convergence are attained by Kernel estimators.},
  ISSN                     = {00359246},
  Owner                    = {nina},
  Publisher                = {[Royal Statistical Society, Wiley]},
  Timestamp                = {2017.12.20},
  Url                      = {http://www.jstor.org/stable/2345837}
}

