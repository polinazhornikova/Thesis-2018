@Article{Muller.Stadtmuller1987,
  Title                    = {Estimation of Heteroscedasticity in Regression Analysis},
  Author                   = {Hans-Georg Muller and Ulrich Stadtmuller},
  Journal                  = {The Annals of Statistics},
  Year                     = {1987},
  Number                   = {2},
  Pages                    = {610-625},
  Volume                   = {15},
  Abstract                 = {Consider the regression model Yi = g(ti) + εi, 1 ≤ i ≤ n, with nonrandom design variables (ti) and measurements (Yi) for the unknown regression function g(·). We assume that the data are heteroscedastic, i.e., $E(\varepsilon^2_i) = \sigma^2_i \not\equiv \operatorname{const.}$ and investigate how to estimate σ2 i. If σ2 i = σ2(ti) with a smooth function σ2(·), initial estimators σ̃2 i can be improved by kernel smoothers and the resulting class of estimators is shown to be uniformly consistent. These estimates can be used to improve the estimation of the regression function g itself in parametric and nonparametric models. Further applications are suggested.},
  ISSN                     = {00905364},
  Owner                    = {nina},
  Publisher                = {Institute of Mathematical Statistics},
  Timestamp                = {2017.12.20},
  Url                      = {http://www.jstor.org/stable/2241329}
}

